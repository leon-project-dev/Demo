<dx:ThemedWindow 
    x:Class="WPFComamnd.MainWindow"
    xmlns:local="clr-namespace:WPFComamnd"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="MainWindow" Height="800" Width="1000">
    <dx:ThemedWindow.CommandBindings>
        <CommandBinding Command="{x:Static local:DataCommandBinding.DoSomething}" Executed="CommandBinding_Executed"></CommandBinding>
    </dx:ThemedWindow.CommandBindings>
    <Grid>
        <Button Content="按钮" Command="{x:Static local:DataCommandBinding.DoSomething}"></Button>
    </Grid>
</dx:ThemedWindow>
